<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.food.project.mapper.CallListMapper">
	<select id="getCallList" resultType="com.food.project.domain.CallListVO">
		select * from call_list where truck_code=#{truck_code} and email=#{email}
		order by festival_date desc
	</select>
	<insert id="insertCallList" parameterType="com.food.project.domain.CallListVO">
		insert into call_list(email,truck_code,place,festival_date,price,progress,festival_name,deposit_date,advance_rate)
		values(#{email},#{truck_code},#{place},#{festival_date},#{price},#{progress},#{festival_name},#{deposit_date},#{advance_rate})
	</insert>
</mapper>